{
    "name": "swew/cli",
    "description": "Cli package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AVIL13",
            "email": "avil@swew.app"
        }
    ],
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "SwewCli\\": "src/"
        },
        "exclude-from-classmap": [
            "spec",
            "/Tests/",
            "/test/",
            "/tests/"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "laravel/pint": "^v1.3.0",
        "phpstan/phpstan": "^1.9.4",
        "swew/test": "^v1.0.9",
        "vimeo/psalm": "^4.30.0"
    },
    "scripts": {
        "test": "t",
        "serve": "php -S localhost:8080 -t public/",
        "static": "./vendor/bin/psalm --no-cache",
        "stan": "vendor/bin/phpstan analyse src",
        "code-check": "./vendor/bin/pint",
        "check": [
            "@static",
            "@stan",
            "@code-check"
        ]
    }
}
